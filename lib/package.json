{
  "name": "@dev.mohe/indexeddb",
  "version": "0.0.2",
  "description": "Make a database interface available that works in the browser and in nodejs",
  "exports": {
    ".": {
      "browser": "./build/browser.js",
      "node": "./build/node.js",
      "default": "./build/browser.js"
    },
    "./src/interface": "./build/interface.js"
  },
  "types": "./src/api.d.ts",
  "type": "module",
  "scripts": {
    "start": "node --conditions=browser node_modules/snowpack/index.bin.js dev",
    "watch": "tsc --watch",
    "build": "node --conditions=browser node_modules/snowpack/index.bin.js build",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\"",
    "docs": "typedoc --allowJs src/"
  },
  "devDependencies": {
    "@snowpack/plugin-run-script": "^2.1.2",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.11.2",
    "@types/snowpack-env": "^2.3.0",
    "prettier": "^2.1.2",
    "snowpack": "^2.12.0-pre.1",
    "typedoc": "^1.0.0-dev.3",
    "typescript": "^4.1.0-dev.20200921"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohe2015/indexeddb.git"
  },
  "author": "Moritz.Hedtke <Moritz.Hedtke@t-online.de>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/mohe2015/indexeddb/issues"
  },
  "homepage": "https://github.com/mohe2015/indexeddb#readme",
  "optionalDependencies": {
    "mongodb": "^3.6.2"
  },
  "dependencies": {
    "ts-node": "^9.0.0"
  },
  "private": false
}
